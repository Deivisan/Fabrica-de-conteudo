{
  "name": "fabrica-de-conteudo-e-vamos-iterar",
  "version": "1.0.0",
  "description": "Fábrica de Conteúdo Inteligente - Plataforma avançada para geração automatizada de conteúdo multimídia para todas as redes sociais, usando inteligência artificial e automação.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:automation": "node treinamento/tests/automation.test.js",
    "setup": "node scripts/quick-start.js",
    "setup:sessions": "node treinamento/index.js --setup",
    "check:sessions": "node treinamento/index.js --check",
    "generate:image": "node treinamento/examples/generate-image.js",
    "generate:text": "node treinamento/examples/generate-text.js",
    "generate:campaign": "node treinamento/examples/full-campaign.js",
    "docker:build": "docker build -t mcp-platform .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 -v ./browser-data:/app/browser-data mcp-platform",
    "docker:compose": "docker-compose up -d",
    "docker:setup": "docker-compose --profile setup run mcp-setup",
    "playwright:install": "npx playwright install chromium --with-deps",
    "playwright:install:all": "npx playwright install --with-deps"
  },
  "keywords": [
    "ai-content",
    "content-generation",
    "marketing-automation",
    "social-media",
    "playwright",
    "nodejs",
    "docker",
    "markdown-parser"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "gray-matter": "^4.0.3",
    "uuid": "^9.0.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "sharp": "^0.32.6",
    "puppeteer": "^21.4.1",
    "openai": "^4.16.1",
    "axios": "^1.5.1",
    "cheerio": "^1.0.0-rc.12",
    "js-yaml": "^4.1.0",
    "lowdb": "^7.0.1",
    "moment": "^2.29.4",
    "fs-extra": "^11.1.1",
    "async": "^3.2.4",
    "@anthropic-ai/sdk": "^0.12.4",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@google/generative-ai": "^0.1.3",
    "groq-sdk": "^0.5.0",
    "zod": "^3.22.4",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/fabrica-de-conteudo-e-vamos-iterar.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/fabrica-de-conteudo-e-vamos-iterar/issues"
  },
  "homepage": "https://github.com/seu-usuario/fabrica-de-conteudo-e-vamos-iterar#readme"
}